
<h1>New Documentary</h1>

<%= form_with(model: @documentary, url: documentary_index_path) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :synopsis %>
    <%= form.text_area :synopsis %>
  </div>

  <div>
    <%= form.label :director %>
    <%= form.text_field :director %>
  </div>

  <%= form.submit "Create Documentary" %>
<% end %>
